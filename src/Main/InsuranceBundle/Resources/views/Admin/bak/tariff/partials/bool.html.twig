{% for key, value in entry %}

  {% if value is iterable %}
    {% for subValue in value %}
      {% for deepKey, deepValue in subValue %}
        {{ deepKey }}: {{ deepValue }}
      {% endfor %}
      {% if not loop.last %},{% endif %}<br>
    {% endfor %}
  {% else %}
    {% if key == 'bool' %}
      {% if value == 1 %}
        <i class="fas fa-check"></i>
      {% else %}
        <i class="fas fa-times"></i>
      {% endif %}
      <br>
    {% else %}
      {{ key }}: {{ value }}
    {% endif %}
  {% endif %}
{% endfor %}
