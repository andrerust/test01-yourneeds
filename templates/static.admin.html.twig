<!DOCTYPE html>
<html lang="de">
<head>
   <meta charset="utf-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1">

   <link rel="apple-touch-icon" sizes="180x180" href="/build/static/images/apple-touch-icon.png">
   <link rel="icon" type="image/png" sizes="32x32" href="/build/static/images/favicon-32x32.png">
   <link rel="icon" type="image/png" sizes="16x16" href="/build/static/images/favicon-16x16.png">
   <link rel="manifest" href="/build/static/images/site.webmanifest">
   <link rel="mask-icon" href="/build/static/images/safari-pinned-tab.svg" color="#74b959">
   <link rel="shortcut icon" href="/build/static/images/favicon.ico">
   <meta name="msapplication-TileColor" content="#74b959">
   <meta name="msapplication-config" content="/build/static/images/browserconfig.xml">
   <meta name="theme-color" content="#ffffff">

   <meta property="og:image:width" content="1600">
   <meta property="og:image:height" content="1365">
   <meta property="og:description"
         content="Mit YOURneeds hast du deinen Versicherungsmanager in deiner Hosentasche oder Handtasche und schonst die Umwelt. Erstaunlich, was wir gemeinsam sparen k&ouml;nnen!">
   <meta property="og:title" content="Versichere dich selbst! &ndash; YOURneeds GmbH">
   <meta property="og:url" content="https://www.yourneeds.de">
   <meta property="og:image" content="https://www.yourneeds.de/build/static/images/family.jpg">

   <meta name="description"
         content="Mit YOURneeds hast du deinen Versicherungsmanager in deiner Hosentasche oder Handtasche und schonst die Umwelt. Erstaunlich, was wir gemeinsam sparen k&ouml;nnen!">

   <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate"/>
   <meta http-equiv="Pragma" content="no-cache"/>
   <meta http-equiv="Expires" content="0"/>

   <title>
      {% block title %}Versichere Dich selbst! – YOURneeds GmbH{% endblock %}
   </title>
   {% block stylesheets %}
      <link rel="stylesheet" href="/build/yourneeds-base.css"/>
      <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-j8y0ITrvFafF4EkV1mPW0BKm6dp3c+J9Fky22Man50Ofxo2wNe5pT1oZejDH9/Dt" crossorigin="anonymous">
   {% endblock %}

   {% block javascripts %}
      <script src="/build/libs/popper.min.js" crossorigin="anonymous"></script>
      <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
      <![endif]-->
      <script src="/build/yourneeds-base.js"></script>

      <!DOCTYPE html>

   {% endblock %}
    <script>
    var _paq = _paq || [];
    _paq.push(["trackPageView"]);
    _paq.push(["enableLinkTracking"]);
    _paq.push(["enableHeartBeatTimer", 5]);
    (function() {
        var u="https://piwik.bitandblack.com/";
        _paq.push(["setTrackerUrl", u+"piwik.php"]);
        _paq.push(["setSiteId", "28"]);
        var d=document, g=d.createElement("script"), s=d.getElementsByTagName("script")[0];
        g.type="text/javascript"; g.async=true; g.defer=true; g.src=u+"piwik.js"; s.parentNode.insertBefore(g,s);
    })();
    </script>
</head>
<body>
{% if app.environment == 'dev2' %}
   <div id="dev-layer"
        style="bottom: 10%; left: 10%; width: 80%; height: 50%; position: fixed; background-color: rgba(255, 255, 255, .9); z-index: 9999; padding: 1em;">
      <ul>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_USER') %}
               <a href="{{ path('security_logout') }}" style="color: inherit;" data-yn-toggle-mobilemenu>Abmelden</a>
            {% else %}
               <a href="{{ path('security_login') }}" style="color: inherit;" data-yn-toggle-mobilemenu>Anmelden</a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_USER') %}
               <a href="{{ path('manager_choose_survey', {
               }) }}">
                  || Check Tarif&nbsp;
               </a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_USER') %}
               <a href="{{ path('manager_show_result', {
                  'id': user.id,
                  'type': 'pli'
               }) }}">
                  || Resultatseite PHV&nbsp;
               </a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_USER') %}
               <a href="{{ path('manager_profile', {
                  'id': user.id
               }) }}">
                  || Profil&nbsp;
               </a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_USER') %}
               <a href="{{ path('damage_type_decision', {
                  'id': user.id
               }) }}">
                  || Schaden melden&nbsp;
               </a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_USER') %}
               <a href="{{ path('contracts', {
                  'id': user.id
               }) }}">
                  || Meine Verträge
               </a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_ADMIN') %}
               <a href="{{ path('admin_managers') }}">
                  || Admin-Alle Nutzer
               </a>
            {% endif %}
         </li>
         <li>
            {% if user.id is defined and not (user.id is null) and is_granted('ROLE_ADMIN') %}
               <a href="{{ path('admin_manager', {
                  'id': user.id
               }) }}">
                  || Admin-Nutzer
               </a>
            {% endif %}
         </li>
         <li>
            <div id="dimensions"></div>
         </li>
      </ul>
   </div>
{% endif %}
<div class="col-menu col-menu--small2 hidden-xs hidden-sm">
    {% include 'includes/manager.navigation.html.twig' %}
</div>
{% block content %}{% endblock %}
{% set _path = app.request.get('_controller')|split(':') %}
{% set controller = _path[0] %}

<!-- Controller: {{ controller }} -->

{% if is_granted('ROLE_USER') %}
    <!-- is_granted: true -->
{% else %}
    <!-- is_granted: false -->
{% endif %}

{% if controller != 'AppBundle\Controller\MainController' and is_granted('ROLE_USER') %}
    <div class="mobilerow">
        <a href="{{ path('contracts') }}">
            <i class="fas fa-file-signature fa-fw"></i>
        </a>
        <a href="{{ path('manager_choose_survey') }}">
            <i class="fas fa-bullseye-pointer fa-fw"></i>
        </a>
        <a href="{{ path('contract_new') }}">
            <i class="fas fa-plus-circle fa-fw"></i>
        </a>
        <a href="{{ path('show_damages') }}">
            <i class="fas fa-car-crash fa-fw"></i>
        </a>
        <a href="javascript:void(0);" data-yn-toggle-col-menu>
            <i class="fas fa-bars fa-fw"></i>
        </a>
    </div>
{% endif %}
</body>
</html>
